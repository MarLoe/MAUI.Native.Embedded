<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <Target Name="_InitMauiNativeAssetsPaths">
    <PropertyGroup>
      <_MauiNativeAssetsPathRoot Condition="'$(_MauiNativeAssetsPathRoot)' == ''">$(MSBuildProjectDirectory)/$(IntermediateOutputPath)maui-native-assets/</_MauiNativeAssetsPathRoot>
    </PropertyGroup>
  </Target>

  <PropertyGroup>
    <_InitMauiNativeEmbeddedDependsOn>
      $(_InitMauiNativeEmbeddedDependsOn)
      _InitMauiNativeAssetsPaths;
    </_InitMauiNativeEmbeddedDependsOn>

    <_ResolveMauiNativeEmbeddedDependsOn>
      $(_ResolveMauiNativeEmbeddedDependsOn)
    </_ResolveMauiNativeEmbeddedDependsOn>

    <_ProcessMauiNativeEmbeddedDependsOn>
      $(_ProcessMauiNativeEmbeddedDependsOn);
    </_ProcessMauiNativeEmbeddedDependsOn>

    <ProcessMauiNativeEmbeddedDependsOn>
      $(ProcessMauiNativeEmbeddedDependsOn);
    </ProcessMauiNativeEmbeddedDependsOn>

    <VerifyMauiNativeEmbeddedDependsOn>
      $(VerifyMauiNativeEmbeddedDependsOn);
    </VerifyMauiNativeEmbeddedDependsOn>
  </PropertyGroup>


  <Target Name="_InitMauiNativeEmbedded" DependsOnTargets="$(_InitMauiNativeEmbeddedDependsOn)" AfterTargets="AfterResolveReferences" />

  <Target Name="_ResolveMauiNativeEmbedded" DependsOnTargets="$(_ResolveMauiNativeEmbeddedDependsOn)" />

  <Target Name="_ProcessMauiNativeEmbedded" DependsOnTargets="$(_ProcessMauiNativeEmbeddedDependsOn)" />

  <Target Name="ProcessMauiNativeEmbedded" BeforeTargets="ProcessMauiAssets" DependsOnTargets="$(ProcessMauiNativeEmbeddedDependsOn)" />

  <Target Name="VerifyMauiNativeEmbedded" AfterTargets="Build" DependsOnTargets="$(VerifyMauiNativeEmbeddedDependsOn)" />

  <!-- This target is for debug purposes. See README.md for information -->
  <Target Name="BuildNativeEmbedded">
    <CallTarget Targets="ProcessMauiNativeEmbedded" />
  </Target>

</Project>